<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="encrypted_prefs"/>
        <exclude domain="database" path="cc_monitor.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Rules for device-to-device transfer -->
        <exclude domain="sharedpref" path="encrypted_prefs"/>
    </device-transfer>
</data-extraction-rules>